{
  "appId": "com.coresfragrancias.desktop",
  "productName": "CoresFragrancias",
  "directories": {
    "output": "dist"
  },
  "files": [
    "**/*"
  ],
  "extraResources": [
    {
      "from": "../app.py",
      "to": "app.py"
    }
  ],
  "win": {
    "target": [
      "nsis"
    ]
  },
  "mac": {
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "target": [
      "AppImage"
    ]
  }
}